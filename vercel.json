{
    "version": 2,
    "builds": [
      { "src": "next.config.js", "use": "@vercel/next" },
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "package.json", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  